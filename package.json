{
  "name": "count-visit-api",
  "version": "1.0.0",
  "description": "AWS Lambda API for tracking website visit counts",
  "type": "module",
  "main": "src/handler.js",
  "scripts": {
    "build": "sam build",
    "deploy": "sam deploy --guided",
    "deploy:dev": "sam deploy --config-env dev",
    "deploy:prod": "sam deploy --config-env prod",
    "local": "sam local start-api",
    "logs": "sam logs -n CountVisitsFunction --tail",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "validate": "sam validate",
    "delete": "sam delete"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "esbuild": "^0.25.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "aws",
    "lambda",
    "sam",
    "visit-tracking",
    "analytics"
  ],
  "author": "Szilvia",
  "license": "MIT"
}
